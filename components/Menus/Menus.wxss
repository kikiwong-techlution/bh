/**Meuns.wxss**/
.menus {
  margin:15rpx 10rpx 0 10rpx;
  padding: 15rpx;
  border-radius: 20rpx;
  background: #fff;
  border: 1px solid whitesmoke;
}
.menus-list{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  align-items: center;
}
.menus-list-item{
  height: 100rpx;
  flex: 0 1 25%;
  text-align: center;
  margin-bottom: 20rpx; 
  margin-top: 20rpx;
}
.menus-list-item-image{
  height: 60rpx;
  width: 60rpx;
}
.menus-list-item-view{
  font-size: 24rpx;
  font-weight: 400;
}
